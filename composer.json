{
    "name": "cjtterabytesoft/yii2-theme-adminator-skeleton",
    "description": "Yii21 Theme: Skeleton Dashboard Adminator Admin.",
    "type": "yii2-extension",
    "keywords": ["yii21","themes","yii21-themes", "Skeleton", "Dashboard", "Adminator", "Admin"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Wilmer Arambula",
            "email": "cjtterabytellc@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2-bootstrap4": "1.0.x-dev",       
        "bower-asset/bootbox": "*",
        "bower-asset/Font-Awesome": "*",
        "bower-asset/ionicons": "*",
        "bower-asset/themify-icons": "*",
        "himiklab/yii2-recaptcha-widget" : "*"
    },
    "require-dev": {
        "yiisoft/yii2-coding-standards": ">=2.0.3"
    },    
    "autoload": {
        "psr-4": {
            "cjtterabytesoft\\theme\\adminskeleton\\": "./src/adminskeleton"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cjtterabytesoft\\theme\\adminskeleton\\": "./src/adminskeleton"
        }
    },    
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "bootstrap": "cjtterabytesoft\\theme\\adminskeleton\\Bootstrap",
        "branch-alias": {
          "dev-master": "0.0.1-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}